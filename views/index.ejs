<!DOCTYPE HTML>
<html lang="en" ng-app="app">
  <head>
    <meta charset="utf-8">
    <link href="//netdna.bootstrapcdn.com/twitter-bootstrap/2.1.1/css/bootstrap.no-responsive.no-icons.min.css" rel="stylesheet"/>
    <link href="/css/skg.css" rel="stylesheet"/>
  </head>
  <body>
    <div id="container" class="container-fluid">
      <div class="row">
        <div class="span3">
          <h1>photography by SKG</h1>
        </div>
        <div class="span9">
          <div id="image-container">

          </div>
        </div>
      </div>
    </div>
    <script type="text/javascript" src="https://ajax.googleapis.com/ajax/libs/jquery/1.8.2/jquery.min.js"></script>
    <script type="text/javascript" src="https://ajax.googleapis.com/ajax/libs/angularjs/1.0.2/angular.min.js"></script>
    <script type="text/javascript" src="/vendor/jquery.backstretch-min.js"></script>
    <script type="text/javascript" src="app.js"></script>
    <script type="text/javascript" src="controllers.js"></script>
    <script type="text/javascript">
      $(document).ready(function() {
        setTimeout(function() {
          $.backstretch(["/images/astoria.jpg", "/images/aspen.jpg"], {fade: 500, duration: 4000});
        }, 1000);

        $(document).on("backstretch.show", function(e) {
          console.log(e);
        });
      });
    </script>
  </body>
</html>